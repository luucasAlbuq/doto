<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seudoto"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:targetPackage="com.example.seudoto" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/estetoscopio_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="activity.BuscarActivity"
            android:label="@string/title_activity_buscar" >
        </activity>
        <activity
            android:name="activity.ListaProfissionaisActivity"
            android:label="@string/title_activity_lista_profissionais" >
        </activity>
        <activity
            android:name="activity.DetalhesProfissionalActivity"
            android:label="@string/title_activity_detalhes_profissional" >
        </activity>
        <activity
            android:name="activity.CadastrarProfissionalActivity"
            android:label="@string/title_activity_cadastrar_profissional" >
        </activity>

        <uses-library android:name="android.test.runner" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />

        <activity android:name="com.facebook.LoginActivity" >
        </activity>
        <activity
            android:name="activity.AvaliacaoActivity"
            android:label="@string/title_activity_avaliacao" >
        </activity>
        <activity
            android:name="activity.ComentariosActivity"
            android:label="@string/title_activity_comentarios" >
        </activity>
    </application>

</manifest>